{
    int i,j,n,a=69;
        printf("enter no of rows");
        scanf("%d",&n); // for n=5
     for(i=1;i<=n;i++)
     {
         for(j=1;j<=i;j++)
         {

            printf("%c",a+j-1);
         }

          printf("\n");
               a--;


     }



}

Q6.


#include<stdio.h>
void main(){
    for(int i=1;i<=4;i++){
        int k=i;
        for(int j=4-i;j>0;j--){
            printf(" ");
        }
        for (int l = 0; l < i; l++)
        {
            printf("%d",k);
            k++;
        }
        k=k-2;
        while(k>=i){
            printf("%d",k);
            k--;
        }
        printf("\n");

    }
}




Q7.


#include<stdio.h>
void main(){
    for(int i=1;i<=4;i++){
        int k=i;
        for(int j=4-i;j>0;j--){
            printf(" ");
        }
        while (k>0)
        {
            printf("%d",k);
            k--;
        }
        k=k+2;
        while(k<=i){
            printf("%d",k);
            k++;
        }
        printf("\n");

    }
}




Q8.


#include<stdio.h>
void main()
{
    int i,j,n,k,a=0;
        printf("enter no of rows");
        scanf("%d",&n); //for n=4
     for(i=1;i<=n;i++)
     {
    for(j=1;j<=n-i;j++)
    {

            printf(" ");
    }
        for(k=1;k<=((2*i)-1) ;k++)
{
           printf("*");

    }
    printf("\n");

     }



}


Q9.

#include<stdio.h>
void main()
{
    int i,j,n,k;
        printf("enter no of rows");
        scanf("%d",&n); // for n=5
     for(i=0;i<=n/2;i++)
     {
         for(j=0;j<(n/2)-i;j++)
         {
             printf(" ");
         }
         for(k=0;k<(2*i)+1;k++)
         {
             printf("*");
         }
         printf("\n");
     }
     for(i=(n/2)-1;i>=0;i--)
     {
         for(j=0;j<(n/2)-i;j++)
         {
             printf(" ");
         }
         for(k=0;k<(2*i)+1;k++)
         {
             printf("*");
         }
         printf("\n");



}
}



Q10.


#include<stdio.h>
void main()
{
    int i,j,n,k;
        printf("enter no of rows");
        scanf("%d",&n); // for n=5

     for(i=(n/2);i>=0;i--)
     {
         for(j=0;j<(n/2)-i;j++)
         {
             printf(" ");
         }
         for(k=0;k<(2*i)+1;k++)
         {
             printf("*");
         }
         printf("\n");



}

for(i=1;i<=n/2;i++)
     {
         for(j=0;j<(n/2)-i;j++)
         {
             printf(" ");
         }
         for(k=0;k<(2*i)+1;k++)
         {
             printf("*");
         }
         printf("\n");
     }
}




Q11.


#include<stdio.h>
void main(){
    int n;
    scanf("%d",&n);
    int a=n-1;
    for(int i=1;i<=n;i++){
        int k=1;
        while (k<=i)
        {
            printf("%d",k);
            k++;
        }
        k=k-1;
        for(int j=1;j<=((2*a));j++){
            printf(" ");
            
        }
        while (k>=1){
            printf("%d",k);
            k--;
        }
        printf("\n");
        a--;
        
    }
}




Q12.

#include<stdio.h>
void main(){
    int n;
    scanf("%d",&n);
    int a=0;
    for(int i=n;i>=1;i--){
        int k=1;
        while (k<=i)
        {           
            printf("%d",k);
            k++;
        }
        if(i==n){
        k=k-2;
        }
        else{
            k=k-1;
        }
        for(int j=1;j<=((2*a)-1);j++){
            printf(" ");
        }
        while (k>=1)
        {
            printf("%d",k);
            k--;
        }
        
        printf("\n");
        a++;


    }
}








