Q1.

#include<stdio.h>
void main()
{
    int i,j,a;
    for(i=1;i<=4;i++)
    {
        for(j=1;j<=i;j++)
        {
            a=i+j-1;
            printf("%d",a);
        }
        printf("\n");
    }
}


